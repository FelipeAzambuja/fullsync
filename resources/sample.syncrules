START RULESET UPLOAD
	USE RULEFILES SOURCE
	USE DIRECTION DESTINATION
	USE RECURSION YES
	USE RECURSIONONIGNORE YES

	APPLY IGNORERULES YES
	APPLY TAKERULES YES
	APPLY DELETION DESTINATION

	DEFINE IGNORE "^[.].+"
	DEFINE TAKE "[.]syncrules"
	DEFINE SYNC "length != length"
	DEFINE SYNC "date != date"
END RULESET UPLOAD
