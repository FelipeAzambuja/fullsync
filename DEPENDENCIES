
commons-beanutils.jar
	Dependencies:
		commons-logging.jar

	Used by:
		commons-digester-1.5.jar

commons-betwixt-1.0-alpha-1.jar

commons-collections-3.1.jar
	Dependencies:
		none

	Used by:
		commons-configuration-1.0.jar
		commons-digester-1.5.jar

commons-configuration-1.0.jar
	Dependencies:
		*Core: Java 1.3, commons-collections, commons-lang
		 ConfigurationFactory: commons-logging, commons-digester, JDK 1.4 or xml-apis
		 DatabaseConfiguration: commons-logging, JDBC 3.0 (Java 1.4 or jdbc2_0-stdext.jar)
		*XMLConfiguration, HierarchicalXMLConfiguration: Java 1.4 or (xml-apis + xerces)
		 JNDIConfiguration: commons-logging
		 ConfigurationDynaBean: commons-beanutils, commons-logging

	Uses:
		loading properties from file

commons-cli-1.0.jar
	Dependencies:
		none

	Uses:
		initial startup of application

commons-digester-1.5.jar
	Dependencies:
		commons-beanutils.jar (1.x)
		commons-collections-3.1.jar
		commons-logging.jar (1.0.x)
	
	Uses:
		none

